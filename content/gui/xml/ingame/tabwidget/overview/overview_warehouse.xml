<?xml version="1.0"?>
<!-- Overview tab for warehouses and storages -->
<Container size="260,400" name="warehouse_and_storage_overview">
<TabBG amount="5" />
<Icon name="player_emblem" position="25,50" />

<Container position="0,70" size="245,300">

	<HBox position="22,7"  min_size="170,20"  size="230,20">
	<Spacer min_size="0"  max_size="50" />
		<RenameLabel name="headline" helptext="Click to change the name of your settlement" />
	</HBox>

	<Icon image="content/gui/images/background/hr.png" position="20,30" />

	<HBox size="225,20" max_size="225,20" position="25,43">
		<Label name="name_label" text="Name:" />
		<Label name="name" />
	</HBox>

	<HBox min_size="125,20" max_size="225,20" position="25,65">
		<Icon name="running_costs_label" helptext="Running costs"
			image="content/gui/icons/resources/running_costs.png" />
		<Label name="running_costs"
			helptext="Running costs" />
		<Spacer max_size="50" />
		<Icon name="collector_utilization_label"  helptext="Collector utilization"
			image="content/gui/icons/resources/cap_util.png" />
		<Label name="collector_utilization" helptext="Collector utilization"
			comment="Percentage describing how busy the collectors were (100% = always going for / already carrying full load of goods)" />
	</HBox>
</Container>
</Container>
